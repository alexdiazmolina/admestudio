ue = evt.target.value;\n      tempValue = evt.target.value;\n      // TODO: Agregar validaciones ac√°\n      this.value = tempValue;\n      this.$emit('input', value);\n    },\n    onFocus(value) {\n      this.focused = true;\n      this.$emit('focus', value);\n    },\n    onBlur(value) {\n      this.focused = false;\n      this.$emit('blur', value);\n    }\n  }\n};\n</script>\n<style></style>\n"]}]}